{
	"configurations": [
		{
			"name": "Mac",
			"includePath": [
				"${workspaceFolder}/**",
				"/opt/X11/include"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"macFrameworkPath": [
				"/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
			],
			"compilerPath": "/usr/bin/g++",
			"cStandard": "c11",
			"cppStandard": "c++14",
			"intelliSenseMode": "macos-clang-arm64"
		}
	],
	"version": 4
}
